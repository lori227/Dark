<?xml version='1.0' encoding='utf-8' ?>
<Config>
	<!-- 认证服务器地址 -->
	<Auth Address="Http://127.0.0.1:7001/auth"/>

	<!-- 网络压缩等级 -->
	<Net Compress="0"/>
	
	<!-- 基本信息 -->
	<!-- Count 最大机器人数量 -->
	<!-- Interval 登录间隔时间, 单位: 毫秒 -->
	<Login Account="Robot" Count="1" Interval="5000"/>

	<!-- 日志开关 -->
	<!-- Data 属性更新日志 -->
	<Log Data="1"/>

</Config>

